<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <TextView
        android:id="@+id/keyTextTv"
        android:layout_width="@dimen/dp_0"
        android:layout_height="match_parent"
        android:textSize="@dimen/sp_16"
        android:textColor="@color/textColor_333333"
        android:gravity="center_vertical|start"
        android:maxLines="1"
        android:singleLine="true"
        android:paddingEnd="@dimen/dp_5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toStartOf="@id/valueTextTv"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.0"/>

    <TextView
        android:id="@+id/valueTextTv"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:textSize="@dimen/sp_14"
        android:textColor="@color/textColor_666666"
        android:gravity="center_vertical|end"
        android:maxLines="1"
        android:singleLine="true"
        android:paddingStart="@dimen/dp_5"
        app:layout_constraintStart_toEndOf="@id/keyTextTv"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="1.0"/>

</androidx.constraintlayout.widget.ConstraintLayout>