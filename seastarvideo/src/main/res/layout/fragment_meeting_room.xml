<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/black_363b41"
    tools:context=".meeting.room.MeetingRoomFragment">

    <com.freewind.seastarvideo.ui.ClickFrameLayout
        android:id="@+id/contentSecFl"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/topBarI"
        app:layout_constraintBottom_toBottomOf="parent"/>

    <include
        android:id="@+id/topBarI"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        layout="@layout/include_meeting_room_topbar"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"/>

    <include
        android:id="@+id/bottomToolbarI"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_56"
        layout="@layout/include_meeting_room_bottom_toolbar"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"/>

</androidx.constraintlayout.widget.ConstraintLayout>